<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    
<mapper namespace="VolApply-Mapper">


<insert id="insertVolApply" parameterType="volApply">

insert into
vol_apply(va_no,mem_id,is_approved,is_join,vb_no)
values(va_no_seq,#{mem_id},#{is_approved},#{is_join},#{vb_no})

</insert>



</mapper>