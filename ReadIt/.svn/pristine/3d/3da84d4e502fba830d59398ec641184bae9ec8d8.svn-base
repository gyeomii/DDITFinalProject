	<div class="container">
		<div class="content-header mb-2">
			<h1>회원 가입</h1>
		</div>
		<div class="content-body">
		<div class="row">
			<!-- Left Text-->
			<div class="col-4 align-items-center">
			<div style="margin-top:30px;"></div>
			    <div class="w-100 d-lg-flex align-items-center justify-content-center">
			        <img src = "<%=request.getContextPath()%>/resources/images/책박이.PNG" style="width: 100%; height: 100%;"  >
			    </div>
			</div>
			<!-- /Left Text-->
			<!-- Register-->
			<div class="col-8 align-items-center auth-bg ">
            	<div class="card-body">
                	<div style="margin-top:30px;"></div>
	                <div class="row">
	                    <div class="mb-1 col-6">
	                    	<label class="form-label" for="id">Check User ID and confirm</label>
	                  		<div class="input-group">
	                  		    <input type="text" class="form-control" placeholder="Check Your ID and confirm" aria-describedby="button-addon2" />
	                      		<button class="btn btn-outline-primary" id="button-addon2" type="button"><i data-feather='search'></i></button>
	                      		<button class="btn btn-outline-primary" id="button-addon2" type="button"><i data-feather='check'></i></button>
	                      	</div>
	                    </div>     
	                    <div class="mb-1 col-6">
	                   		<label class="form-label" for="id">Check Admin</label>
	             		    <div class="input-group">
	               		      	<input type="text" class="form-control" placeholder="Click the button If you are an admin" aria-describedby="button-addon2" />
	                  		    <div class="input-group-text">
	                            	<div class="form-check">
	                                	<input class="form-check-input" type="checkbox" id="inputCheckbox">
	                             	</div>
	                        	</div>
	                   		</div>
	            		</div>   
	            	</div>  
					<div class="row">
	                    <div class="mb-1 col-md-6">
	                    	<label class="form-label" for="password">Password</label>
	                        <div class="input-group input-group-merge form-password-toggle">
	                        	<input type="password" name="password" id="password" class="form-control" placeholder="&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;" />
	                            <span class="input-group-text cursor-pointer"><i data-feather="eye"></i></span>
	                        </div>
	                    </div>
	                    <div class="mb-1 col-md-6" >
	                    	<label class="form-label" for="confirm-password">Confirm Password</label>
	                        <div class="input-group input-group-merge form-password-toggle">
	                            <input type="password" name="confirm-password" id="confirm-password" class="form-control" placeholder="&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;" />
	                            <span class="input-group-text cursor-pointer"><i data-feather="eye"></i></span>
	                        </div>
	                    </div>
	            	</div>   
	            	<div class="row">
	                    <div class="mb-1 col-md-6">
	                        <label class="form-label" for="first-name">Name</label>
	                        <input type="text" name="name" id="name" class="form-control" placeholder="" />
	                    </div>
						
	                    <div class="mb-1 col-md-6" >
	                    	<label class="form-label" for="gender">Gender</label>
	                    	</br>
		                    <div class="form-check form-check-inline">
		                    	<input type="radio" class="form-check-input" id="m" value="M" name="gender">
		                    	<label class="form-check-label" for="m">Male</label>
		                    </div>
		                    <div class="form-check form-check-inline">
		                    	<input type="radio" class="form-check-input" id="f" value="F" name="gender">
		                    	<label class="form-check-label" for="f">Female</label>
		                    </div>
	                    </div>
	                </div>
					<!-- Flatpickr Starts -->
					<section id="flatpickr">
						<div class="row">
							<div class="col-md-6 mb-1">
						    	<label class="form-label" for="fp-default">Birth</label>
						        <input type="text" id="fp-default" class="form-control flatpickr-basic" placeholder="YYYY-MM-DD"/>
						    </div>
							<div class="mb-1 col-md-6" >
		                        <label class="form-label" for="last-name">Age</label>
		                        <input type="text" name="age" id="age" class="form-control" placeholder="" />
		                    </div>
						</div>
					</section>
					<!-- Flatpickr Ends-->
                    <div class="row">
		                <div class="col-6 mb-1">
		                    <label class="form-label" for="home-address">Home Address</label>
		                    <input type="text" name="home-address" id="home-address" class="form-control" placeholder="Address" />
		                </div>
		                <div class="mb-1 col-md-6" >
	                    	<label class="form-label" for="pin-code">E-mail</label>
                        	<input type="text" name="email" id="email" class="form-control pin-code-mask" placeholder="aaaa123@bbbb.ccc" />
                    	</div>
                    </div>
                    <div class="row">
						<div class="mb-1 col-6">
	                    	<label class="form-label" for="first-name">Interest</label>
	                    	<input type="text" name="insterest" id="interest" class="form-control" placeholder="" />
	                    </div>
	                    <div class="mb-1 col-6" >
				            <label class="form-label" id="basicSelect">MBTI</label>
							<div class="row input-group" style="margin: 0px;">
				               	<select class="col-3 form-select" id="basicSelect">
				               		<option>I</option>
				               		<option>E</option>
				               	</select>
				               	<select class="col-3 form-select" id="basicSelect">
				               		<option>S</option>
				               		<option>N</option>
				               	</select>
				               	<select class="col-3 form-select" id="basicSelect">
				               		<option>F</option>
				               		<option>T</option>
				               	</select>
				               	<select class="col-3 form-select" id="basicSelect">
				               		<option>P</option>
				               		<option>J</option>
				               	</select>
			                </div>
	                    </div>
                    </div>

					<div class="row">
	 					<div class="mb-1 col-md-6">
	                        <label class="form-label" for="mobile-number">Car Number</label>
	                        <input type="text" name="carnumber" id="carnumber" class="form-control mobile-number-mask" placeholder="" />
	                    </div>
	                    <div class="mb-1 col-md-6" >
	     	            	<label class="form-label" for="mobile-number">Mobile number</label>
	                        <input type="text" name="mobile-number" id="mobile-number" class="form-control mobile-number-mask" placeholder="(010) 1234-5678" />
	                    </div>
                    </div>
                    <div class="mb-1 col-6">
	             		<label class="form-label" for="id">Member Image</label>
	      		       	<div class="input-group">
	        		    	<input type="text" class="form-control" placeholder="Image File" aria-describedby="button-addon2" />
	            			<button class="btn btn-outline-primary" id="button-addon2" type="button"><i data-feather='search'></i></button>
	            		</div>
                    </div> 
                </div>
            </div>
        </div>
    </div>
</div>
