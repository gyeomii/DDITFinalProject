<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Stupid-Mapper">
	<insert id="insertStupidBook" parameterType="stupid">
		insert into stupid_book (sb_no, sb_location, sb_regdate, book_kdc)
		values (sb_no_seq.nextval, #{sb_location}, sysdate, #{book_kdc})
	</insert>
	
	<select id="selectStupidBookList" parameterType="stupid">
		select * from stupid_book
	</select>
</mapper>
