            <div class="content-body">
<!--                 <div class="row match-height"> -->
                <div class="row">

                    <!-- Payment Card -->
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <form action="fingerModifyForRegist.do" role="form" class="form" name="registForm" method="post">
                                	<div class="demo-inline-spacing mb-2 col-12" style="margin-left: 70px; ">
					                      <div class="form-check form-check-primary">
					                         <input class="form-check-input" type="radio" name="inlineRadioOptions" id="purchase" value="option1" unchecked="">
					                          <label class="form-check-label" for="purchase">고서/귀중도서</label>
					                      </div>
					                      <div class="form-check form-check-secondary">
					                          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="copyBook" value="option2" unchecked="">
					                          <label class="form-check-label" for="copyBook">정기간행물</label>
					                      </div>
					                      <div class="form-check form-check-success">
					                          <input class="form-check-input" type="radio" name="inlineRadioOptions" id="wishBook" value="option3" unchecked="">
					                          <label class="form-check-label" for="wishBook">참고도서</label>
					                      </div>
					                  </div>
                                    <div class="row">
                                    
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">제목</span>
						                    	<div class="input-group">
						                        <input type="text" id="readonlyInput" readonly="readonly" class="form-control" value="${admin.mem_name }" placeholder="Button on both side" aria-label="Amount">
						                        <button class="btn btn-outline-primary waves-effect" type="button" onclick="OpenWindow('searchBook.do','도서검색', 400,400);">
						                        	<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-search"><circle cx="11" cy="11" r="8"></circle><line x1="21" y1="21" x2="16.65" y2="16.65"></line></svg>
						                        </button>
						                    </div>
	                					</div>	
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">저자</span>
												<input type="text" class="form-control" id="readonlyInput" readonly="readonly" value="${admin.dp_name }">
	                					</div>
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">출판사</span>
												<input type="text" class="form-control" id="readonlyInput" readonly="readonly" value="${admin.ad_position }">
	                					</div>
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">발행연도</span>
												<input type="text" class="form-control" id="readonlyInput" readonly="readonly" value="${admin.mem_phone }">
	                					</div>
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">ISBN</span>
												<input type="text" class="form-control" id="readonlyInput" readonly="readonly" value="${admin.mem_phone }">
	                					</div>
                                        <div class="col-md-6 col-12 mb-1">
						                    	<span class="badge badge-light-secondary" style="margin-bottom: 5px;">사유</span>
												<input type="text" class="form-control" id="readonlyInput" readonly="readonly" value="${admin.mem_phone }">
	                					</div>
                                        <div class="d-grid col-6">
                                            <button type="button" class="btn btn-primary" onclick="regist_go();">등록완료</button>
                                        </div>
                                        <div class="d-grid col-6">
                                            <button type="button" class="btn btn-danger" onclick="window.close();">취소</button>
                                        </div>
