package bookred.member.dto;

public class LockerVO {
	private int lk_no;
	private String mem_cardcode;
	private String locker_num;
	private int lk_ispickup;
	private int rm_no;
	
	public int getLk_no() {
		return lk_no;
	}
	public void setLk_no(int lk_no) {
		this.lk_no = lk_no;
	}
	public String getMem_cardcode() {
		return mem_cardcode;
	}
	public void setMem_cardcode(String mem_cardcode) {
		this.mem_cardcode = mem_cardcode;
	}
	public String getLocker_num() {
		return locker_num;
	}
	public void setLocker_num(String locker_num) {
		this.locker_num = locker_num;
	}
	public int getLk_ispickup() {
		return lk_ispickup;
	}
	public void setLk_ispickup(int lk_ispickup) {
		this.lk_ispickup = lk_ispickup;
	}
	public int getRm_no() {
		return rm_no;
	}
	public void setRm_no(int rm_no) {
		this.rm_no = rm_no;
	}
	
	
}
