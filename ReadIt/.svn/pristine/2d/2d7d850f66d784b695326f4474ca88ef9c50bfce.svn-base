<script>

//컬럼 정의
const columnDefs = [
  {headerName:"회원아이디",field: "index", width:80, cellClass: 'alignCenter', filter:false},
  {headerName:"회원명",field: "isbn", width:150, cellClass: 'alignCenter'},
  {headerName:"전화번호",field: "wish_title",width:550, cellClass:""},
  {headerName:"등급",field: "wish_author",width: 200, cellClass:""},
  {headerName:"포인트",field: "wish_publisher",width: 150, cellClass:""},
  {headerName:"대출권수",field: "wish_regdate",width: 120, cellClass:""},
];
const rowData = [
	  { index: "t1", isbn: "Celica", wish_title: "35000",wish_author:"1000", wish_publisher:"1000", wish_regdate:"1000"}
	];
	
const gridOptions = {
	 columnDefs: columnDefs,
	 rowData: rowData,
	 defaultColDef: {  // 모든 컬럼에 공통으로 주는 옵션 설정
	   sortable:true,
	   filter:true,
	   resizable:true,
	 },
	 pagination:true,
	 //paginationAutoPageSize:true,
	 paginationPageSize:2,  //위 AutoPageSize랑 배타적
};

document.addEventListener('DOMContentLoaded', () => {
    const gridDiv = document.querySelector('#mem_info');
    new agGrid.Grid(gridDiv, gridOptions);
});

function sizeToFit(){
	gridOptions.api.sizeColumnsToFit({defaultMinWidth: 100});
}

var data = {mem_id:'aa'}

function wishListAjax(){
	$.ajax({
		url:"<%=request.getContextPath()%>/main/search/wishbookList/listAjax.do",
		type:"GET",
		data:data,
		//dataType:'application/json; charset=utf-8',
		success:function(data){
			console.log(data);
			for(var i= 0; i<data.length; i++){
				console.log(data[i].wish_regdate);
				data[i].wish_regdate = moment(data[i].wish_regdate).format('YYYY-MM-DD')
				data[i].index = i+1;
				if(data[i].wish_state == 0){
					data[i].wish_state = "신청완료";
				}else if(data[i].wish_state == 1){
					data[i].wish_state = "확인중";
				}else if(data[i].wish_state == 2){
					data[i].wish_state = "승인";
				}else if(data[i].wish_state == 3){
					data[i].wish_state = "입고거절";
				}else if(data[i].wish_state == 4){
					data[i].wish_state = "입고중";
				}else if(data[i].wish_state == 5){
					data[i].wish_state = "입고완료";
				}
			}
			gridOptions.api.setRowData(data);
			sizeToFit();
		},
		error:function(error){
			console.log(error);
		}
	})
}

wishListAjax();
</script>
</body>






