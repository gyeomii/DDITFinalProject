package bookred.member.dto;

import java.util.Date;

public class DeliveryVO {
	private int dv_no;
	private Date dv_date;
	private int rm_no;
	private String dv_address;
	private String dv_tell;
	public int getDv_no() {
		return dv_no;
	}
	public void setDv_no(int dv_no) {
		this.dv_no = dv_no;
	}
	public Date getDv_date() {
		return dv_date;
	}
	public void setDv_date(Date dv_date) {
		this.dv_date = dv_date;
	}
	public int getRm_no() {
		return rm_no;
	}
	public void setRm_no(int rm_no) {
		this.rm_no = rm_no;
	}
	public String getDv_address() {
		return dv_address;
	}
	public void setDv_address(String dv_address) {
		this.dv_address = dv_address;
	}
	public String getDv_tell() {
		return dv_tell;
	}
	public void setDv_tell(String dv_tell) {
		this.dv_tell = dv_tell;
	}
	@Override
	public String toString() {
		return "DeliveryVO [dv_no=" + dv_no + ", dv_date=" + dv_date + ", rm_no=" + rm_no + ", dv_address=" + dv_address
				+ ", dv_tell=" + dv_tell + ", getDv_no()=" + getDv_no() + ", getDv_date()=" + getDv_date()
				+ ", getRm_no()=" + getRm_no() + ", getDv_address()=" + getDv_address() + ", getDv_tell()="
				+ getDv_tell() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + ", toString()="
				+ super.toString() + "]";
	}
	
}
