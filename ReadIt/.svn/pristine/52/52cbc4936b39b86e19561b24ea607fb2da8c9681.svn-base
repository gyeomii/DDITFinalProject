	<!-- BEGIN: content-->
	<div class="container">
		<section class="app-user-view-account">
			<div class="row">
				<!-- User Sidebar -->
				<div class="col-xl-4 col-lg-5 col-md-5 order-1 order-md-0">
					<!-- User Card -->
					<div class="card">
						<div class="card-body">
							<div class="user-avatar-section">
								<div class="d-flex align-items-center flex-column">
									<img class="img-fluid rounded mt-3 mb-2"
										src="<%=request.getContextPath()%>/resources/images/hehe.jpg"
										height="110" width="110" alt="User avatar" />
									<div class="user-info text-center">
										<h4>희진짱</h4>
										<span class="badge bg-light-secondary">책책박사</span>
									</div>
								</div>
							</div>
							<div class="d-flex justify-content-around my-2 pt-75">
								<div class="d-flex align-items-start me-2">
									<span class="badge bg-light-primary p-75 rounded"> <i
										data-feather="book-open" class="font-medium-2"
										style="color: forestgreen"></i>
									</span>
									<div class="ms-75">
										<small>읽은 권수</small>
										<h4 class="mb-0">63권</h4>
									</div>
								</div>
								<div class="d-flex align-items-start">
									<span class="badge bg-light-primary p-75 rounded"> <i
										data-feather="gift" class="font-medium-2" style="color: lightcoral"></i>
									</span>
									<div class="ms-75">
										<small>북 포인트</small>
										<h4 class="mb-0">56점</h4>
									</div>
								</div>
							</div>
							<h4 class="fw-bolder border-bottom pb-50 mb-1">나의 정보</h4>
							<div class="info-container">
								<ul class="list-unstyled">
									<li class="mb-75"><span class="fw-bolder me-25">회원명:</span>
										<span>${loginUser.mem_name} </span></li>
									<li class="mb-75"><span class="fw-bolder me-25">Email:</span>
										<span>${loginUser.mem_email}</span></li>
									<li class="mb-75"><span class="fw-bolder me-25">등급:</span>
										<c:choose>
											<c:when test="${loginUser.mem_rank == 0}">
												<span class="badge bg-light-success">책벌레</span>
											</c:when>
											<c:when test="${loginUser.mem_rank == 1}">
												<span class="badge bg-light-success">책책박사</span>
											</c:when>
											<c:otherwise>
												<span class="badge bg-light-waring">등급이 없습니다.</span>
											</c:otherwise>
										</c:choose></li>
									<!--     <li class="mb-75">
                                                <span class="fw-bolder me-25">Role:</span>
                                                <span>Author</span>
                                            </li> -->
								</ul>
								<div class="d-flex justify-content-center pt-2">
									<a href="javascript:;" class="btn btn-primary me-1"
										data-bs-target="#upgradePlanModal" data-bs-toggle="modal">
										모바일 대출증 </a> <a href="javascript:;" class="btn btn-primary me-1"
										data-bs-target="#editUser" data-bs-toggle="modal"> 개인정보 수정
									</a> <a href="javascript:;"
										class="btn btn-outline-danger suspend-user">회원탈퇴</a>
								</div>
							</div>
						</div>
					</div>
					<!-- /User Card -->
					<!-- Plan Card -->
					<div class="card border-primary">
						<div class="card-body">
							<div class="d-flex justify-content-between align-items-start">
								<span class="badge bg-light-primary">검색 => plan card</span>
								<div class="d-flex justify-content-center">
									<sup class="h5 pricing-currency text-primary mt-1 mb-0">$</sup>
									<span class="fw-bolder display-3 mb-0 text-primary">넣을
										내용 있음 추가 아니면 삭제</span> <sub
										class="pricing-duration font-small-4 ms-25 mt-auto mb-2"></sub>
								</div>
							</div>
							<ul class="ps-1 mb-2">
								<li class="mb-50">10 Users</li>
								<li class="mb-50">Up to 10 GB storage</li>
								<li>Basic Support</li>
							</ul>
							<div
								class="d-flex justify-content-between align-items-center fw-bolder mb-50">
								<span>Days</span> <span>4 of 30 Days</span>
							</div>
							<div class="progress mb-50" style="height: 8px">
								<div class="progress-bar" role="progressbar" style="width: 80%"
									aria-valuenow="65" aria-valuemax="100" aria-valuemin="80"></div>
							</div>
							<span>4 days remaining</span>
							<div class="d-grid w-100 mt-2">
								<button class="btn btn-primary"
									data-bs-target="#upgradePlanModal" data-bs-toggle="modal">
									Upgrade Plan</button>
							</div>
						</div>
					</div>
					<!-- /Plan Card -->
				</div>
				<!--/ User Sidebar -->

				<!-- User Content -->
				<div class="col-xl-8 col-lg-7 col-md-7 order-0 order-md-1">
