<script>
function openWindowSearch(){
	var mem_id = $('#seachMemId').val();
	console.log("mem_id",mem_id);
	
	var url="<%=request.getContextPath()%>/admin/memInfo.do";
	
	
	var data = {mem_id : mem_id};
	if(!mem_id){
		alert("아이디를 입력해주세요;")	
	}
	
	$.ajax({
		url : url,
		type: 'get',
		data : data,
		success : function(data){
			alert(data);
		}
		error : function(error){
			console.log("회원 아이디가 존재하지 않습니다.");
	}
	})
}


