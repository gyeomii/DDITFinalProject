package bookred.member.dto;

import java.util.Date;

public class PointVO {
	private int pt_no;
	private String mem_id;
	private Date pt_date;
	private String pt_type;
	private int pt_isincrease;
	private int pt_sum;
	private int over_no;
	
	public int getPt_no() {
		return pt_no;
	}
	public void setPt_no(int pt_no) {
		this.pt_no = pt_no;
	}
	public String getMem_id() {
		return mem_id;
	}
	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}
	public Date getPt_date() {
		return pt_date;
	}
	public void setPt_date(Date pt_date) {
		this.pt_date = pt_date;
	}
	public String getPt_type() {
		return pt_type;
	}
	public void setPt_type(String pt_type) {
		this.pt_type = pt_type;
	}
	public int getPt_isincrease() {
		return pt_isincrease;
	}
	public void setPt_isincrease(int pt_isincrease) {
		this.pt_isincrease = pt_isincrease;
	}
	public int getPt_sum() {
		return pt_sum;
	}
	public void setPt_sum(int pt_sum) {
		this.pt_sum = pt_sum;
	}
	public int getOver_no() {
		return over_no;
	}
	public void setOver_no(int over_no) {
		this.over_no = over_no;
	}
	
}
