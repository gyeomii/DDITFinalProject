<script>
const columnDefs = [
	  {headerName:"마스터번호",field: "sb_no", width:10,  filter:false,headerCheckboxSelection: true, checkboxSelection: true},
	  {headerName:"No.",field: "index", width:100, cellClass: 'alignCenter', filter:false},
	  {headerName:"처리자",field: "ad_id", width:100, filter:false},
	  {headerName:"도서KDC",field: "book_kdc", width:150, cellClass: 'alignCenter'},
	  {headerName:"엉뚱도서 위치",field: "sb_location", width:300},
	  {headerName:"실제 위치해야할 곳",field: "move_location", width:300},
	  {headerName:"엉뚱등록일",field: "sb_regdate", width:200, cellClass: 'alignCenter'},
	  {headerName:"처리상태",field: "sb_isdone", width:10, cellClass: 'alignCenter', hide:true},
	  {headerName:"처리상태",field: "sb_isdone_kor", width:200, cellClass: 'alignCenter'}
	];
const rowData = [
	];
	
const gridOptions = {
	 columnDefs: columnDefs,
	 rowData: rowData,
	 defaultColDef: {  // 모든 컬럼에 공통으로 주는 옵션 설정
	   sortable:true,
	   filter:true,
	   resizable:true,
	 },
	 pagination:true,
	 rowSelection: 'multiple',
	 //paginationAutoPageSize:true,
	 paginationPageSize:15,  //위 AutoPageSize랑 배타적
};

document.addEventListener('DOMContentLoaded', () => {
    const gridDiv = document.querySelector('#myGrid');
    new agGrid.Grid(gridDiv, gridOptions);
});
</script>
<script>
function milliToDate(millisec){
	var date = new Date(millisec);
	var year = date.getFullYear();
	var month = ("0" + (date.getMonth() + 1)).slice(-2);
	var day = ("0" + date.getDate()).slice(-2);
	    
	return year+"-"+month+"-"+day;
}

function setDataKOR(data){
	for(var i = 0; i < data.length; i++){
		data[i].index = i+1;
		if(data[i].sb_isdone == 0){
			data[i].sb_isdone_kor = "미처리";
		}else if(data[i].sb_isdone == 1){
			data[i].sb_isdone_kor = "처리완료";
		}
		date = milliToDate(data[i].sb_regdate);
		data[i].sb_regdate = date;
	}
	return data;
}
</script>
<script>
function stupidListAjax(){
	$.ajax({
		url:"${webPath}/admin/bookManage/stupidBookAdd/done.do",
		type:"GET",
		data:{},
		//dataType:'application/json; charset=utf-8',
		success:function(data){
			fixData = setDataKOR(data);
			gridOptions.api.setRowData(fixData);
		},
		error:function(error){
			console.log(error);
		}
	})
}
stupidListAjax();
</script>
